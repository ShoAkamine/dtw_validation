labels = c("not similar", "similar")),
orientation_bin = factor(orientation_bin,
levels = c(0, 1),
labels = c("not similar", "similar")),
position_bin = factor(position_bin,
levels = c(0, 1),
labels = c("not similar", "similar")),
### z-score
